{
    "name": "lunarphp/core",
    "description": "Lunar Laravel e-commerce core functionality.",
    "keywords": ["lunar", "laravel", "ecommerce", "e-commerce", "headless", "store", "shop", "cart"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
          "name": "Lunar",
          "homepage": "https://lunarphp.io/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lunar\\": "src",
            "Lunar\\Database\\Factories\\": "database/factories",
            "Lunar\\Database\\Seeders\\": "database/seeders",
            "Lunar\\Database\\State\\": "database/state",
            "Lunar\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lunar\\LunarServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0",
        "spatie/laravel-medialibrary": "^9.0.0|^10.0.0",
        "spatie/laravel-activitylog": "^4.4",
        "laravel/scout": "^9.4|^10.0",
        "cartalyst/converter": "^7.0|^8.0",
        "lunarphp/scout-database-engine": "*",
        "kalnoy/nestedset": "^6.0",
        "doctrine/dbal": "^3.6",
        "spatie/laravel-blink": "^1.6",
        "lukascivil/treewalker": "0.9.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    }
}
